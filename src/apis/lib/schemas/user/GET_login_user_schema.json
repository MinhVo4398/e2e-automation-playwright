{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Login response schema",
	"type": "object",
	"properties": {
		"access_token": {
			"type": "string"
		},
		"token_type": {
			"type": "string",
			"const": "bearer"
		},
		"expires_in": {
			"type": "number",
			"const": 300
		}
	},
	"required": ["access_token", "token_type", "expires_in"],
	"additionalProperties": false
}
